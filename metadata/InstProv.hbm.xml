<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.openmrs.module.sockethl7listener.hibernateBeans">

	<class name="InstProv" table="sockethl7listener_institution_provider">

		<id name="instId" type="int" column="Institution_id">
			<generator class="assigned" />
		</id>

		<property name="providerLastName" type="java.lang.String" column="Provider_last_name"
			length="50" not-null="true" />
		<property name="providerFirstName" type="java.lang.String" column="Provider_first_name"
			not-null="true" length="50" />
		<property name="providerID" type="java.lang.String" column="Provider_id"
			not-null="true" length="50" />

	</class>
	
</hibernate-mapping>